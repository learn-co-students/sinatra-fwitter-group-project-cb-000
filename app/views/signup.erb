<h1>Signup Page</h1>

<form method = "POST" action = "/signup">

  <label> Username: </label>
  <input type = "text" name = 'username' id='user_username'/>

  <label> Email: </label>
  <input type = "text" name = 'email' id='user_email'/>

  <label> Password: </label>
  <input type = "text" name = 'password' id='user_password'/>

  <input type = "submit" name = "Sign Up" id = "signup"/>
</form>
