<h1>Welcome to Fwitter</h1>
<h4>Please signup or signin below:</h4>

<nav>
  <% if logged_in? %>
  
    <a href="/tweets/new"> Create a Tweet </a>
    <a href="/logout" > Log Out </a>
    <p>Welcome, <%=current_user.username%></p>
    
  <% else %>
    <a href="/signup"> Sign Up </a>
    <a href="/login"> Sign In </a>    
  <% end %>
</nav>