<!doctype html>
<html>
  <% if flash.has?(:message) %>
    <%= flash[:message] %>
  <% end %>
  <head>
    <title>Fwitter</title>
    <style>
      .wrapper {margin: auto; width: 70%; height: 85%; background-color: dodgerblue; margin-top: 60px;}
      a {font-size: 20px; font-weight: bold; color: yellow; text-decoration: none;}
      a:hover {color: skyblue;}
      .navbar {overflow: hidden; background-color: midnightblue; position: fixed; top: 0; right: 14%;}
      .navbar a {display: inline-block; color: #f2f2f2; text-align: center; padding: 14px 16px; text-decoration: none; font-size: 17px; border-style: none;}
      .navbar a:hover {background: hotpink; color: black;}
      .navbar a:visited {}
      #left{border-style: solid; border-width: 1.5px; float: left; width: 72%; overflow: auto; background: #404040; height: 100%; padding: 1%; text-align: center;}
      #right{border-style: solid; border-width: 1.5px; float: right; width: 22%; overflow: auto; background: #404040; height: 100%; padding: 1%; text-align: center;}
      #logo {text-align: center;}
      h1 {color: coral;}
      h2 {color: darkorange;}
      h3 {color: yellowgreen;}
      h4 {color: cyan;}
      .clearfix:after {content: "."; display: block; clear: both; visibility: hidden; height: 0; line-height: 0;}
    </style>
  </head>
  <body class="wrapper">
    <div class="navbar">
      <a href="/">Homepage</a>
      <% if logged_in? %>
      <a href="/<%= current_user.slug %>/tweets">Tweets</a>
      <a href="/<%= current_user.slug %>/tweets/new">Create a tweet</a>
      <a href="/logout">Logout</a>
      <% else %>
      <a href="/signup">Sign Up</a>
      <a href="/login">Log In</a>
      <% end %>
    </div>
    <h1 id="logo">
      <img src="data:image/png;base64," alt="Fwitter">
    </h1>
    <div id="right">
      <h2>Users</h2>
      <% User.all.each do |user| %>
        <a href="/<%= user.slug %>/tweets"><%= user.username %><a/><br>
      <% end %>
    </div>
    <div id="left">
      <%=yield%>
    </div>
  </body>
</html>
